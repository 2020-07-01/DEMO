<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.shardingSphere.mapper.UserMapper">

    <resultMap id="base" type="com.shardingSphere.entity.User">
        <id property="id" jdbcType="BIGINT" column="id"/>
        <result property="city" jdbcType="VARCHAR" column="city"/>
        <result property="name" jdbcType="VARCHAR" column="name"/>
    </resultMap>

    <insert id="insert">
        insert into user(id,city,name)
        value(#{id,jdbcType=BIGINT},
        #{city,jdbcType=VARCHAR},
        #{name,jdbcType=VARCHAR})
    </insert>


</mapper>